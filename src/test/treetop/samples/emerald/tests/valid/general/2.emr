! doctype html5

html
  head
    h1 "Emerald programming language"
    metas
      name "keywords",    content "HTML, CSS, XML, XHTML"
      name "description", content "Meta tags example"

    scripts
      src "test.js",    type "text/javascript"
      src "js/temp.js", type "text/javascript"

    styles

  ! test this 
  body
    a "This is some text." (
      id    "test this"
      class "test this class"
    )
      h1 "this is cool."

    section (
      accept "all"
    )
      h2 "this is some sample text"
      h3 "this is some more sample text"
